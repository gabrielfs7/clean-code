{
    "name": "flexy/ftwo",
    "type": "project",
    "description": "Flexy E-Commerce Platform v2",
    "authors": [
        {
            "name": "Bruno Silveira",
            "email": "bruno.silveira@flexy.com.br",
            "role": "Developer"
        },
        {
            "name": "Gabriel Soares",
            "email": "gabriel.soares@flexy.com.br",
            "role": "Developer"
        },
        {
            "name": "Jean Carlos Gonçalves",
            "email": "jean.goncalves@flexy.com.br",
            "role": "Developer"
        },
        {
            "name": "Frederico Dubiel",
            "email": "fred.dubiel@flexy.com.br",
            "role": "Developer"
        },
        {
            "name": "Jean Carlos Silva",
            "email": "jean.silva@flexy.com.br",
            "role": "Developer"
        },
        {
            "name": "Gilberto Theves",
            "email": "gilberto.theves@flexy.com.br",
            "role": "Quality Assurance"
        },
